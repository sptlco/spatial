{
  "expo": {
    "name": "Spatial",
    "slug": "mobile",
    "version": "1.2.2",
    "owner": "sptlco",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "bundleIdentifier": "com.sptlco.spatial",
      "supportsTablet": true
    },
    "android": {
      "package": "com.sptlco.spatial",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "favicon": "./assets/images/favicon.png",
      "bundler": "metro"
    },
    "extra": {
      "eas": {
        "projectId": "f250664d-19f8-46a0-9391-40fe62e21048"
      }
    }
  }
}